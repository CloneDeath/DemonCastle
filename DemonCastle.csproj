    <TargetFramework>net6.0</TargetFramework>
    <EnableDynamicLoading>true</EnableDynamicLoading>
    <RootNamespace>DemonCastle</RootNamespace>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="DemonCastle.Files\**" />
    <Compile Remove="DemonCastle.ProjectFiles\**" />
    <Compile Remove="DemonCastle.Game\**" />
    <Compile Remove="DemonCastle.Editor\**" />
    <Compile Remove="DemonCastle.Navigation\**" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Remove="DemonCastle.Files\**" />
    <EmbeddedResource Remove="DemonCastle.ProjectFiles\**" />
    <EmbeddedResource Remove="DemonCastle.Game\**" />
    <EmbeddedResource Remove="DemonCastle.Editor\**" />
    <EmbeddedResource Remove="DemonCastle.Navigation\**" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="DemonCastle.Editor\DemonCastle.Editor.csproj" />
    <ProjectReference Include="DemonCastle.Game\DemonCastle.Game.csproj" />
    <ProjectReference Include="DemonCastle.ProjectFiles\DemonCastle.ProjectFiles.csproj" />
  </ItemGroup>
</Project>